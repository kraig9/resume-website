<p style="color: green"><%= notice %></p>

<%= render @project %>

<div>
  <%= link_to "Edit this project", edit_project_path(@project) %> |
  <%= link_to "Back to projects", projects_path %>

  <%= button_to "Destroy this project", @project, method: :delete %>
</div>

<h2>Links</h2>
<ul>
  <% @project.links.each do |link| %>
    <li><strong><%= link.link_type %>:</strong> <a href="<%= link.url %>" target="_blank"><%= link.url %></a></li>
  <% end %>
</ul>
